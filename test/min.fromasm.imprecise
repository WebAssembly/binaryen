(module
  (memory 256 256)
  (export "memory" memory)
  (export "floats" $floats)
  (func $floats (param $f f32) (result f32)
    (local $t f32)
    (f32.add
      (get_local $t)
      (get_local $f)
    )
  )
  (func $neg (param $k i32) (param $p i32) (result f32)
    (f32.neg
      (block
        (i32.store
          (get_local $k)
          (get_local $p)
        )
        (f32.load
          (get_local $k)
        )
      )
    )
  )
  (func $bitcasts (param $i i32) (param $f f32)
    (f32.reinterpret/i32
      (get_local $i)
    )
    (f64.promote/f32
      (f32.reinterpret/i32
        (get_local $i)
      )
    )
    (i32.reinterpret/f32
      (get_local $f)
    )
  )
  (func $ctzzzz (result i32)
    (i32.ctz
      (i32.const 4660)
    )
  )
)

;; NOTE: Assertions have been generated by update_lit_checks.py and should not be edited.

;; RUN: foreach %s %t wasm-opt -all --roundtrip -S -o - | filecheck %s

(module
  (memory $mem i64 1)

  (func $get_heap_size (result i64)
    (i64.shl
      (memory.size $mem)
      (i64.const 16)
    )
  )

  (func $grow-heap (result i64)
    (memory.grow $mem
      (i64.const 32)
    )
  )
)

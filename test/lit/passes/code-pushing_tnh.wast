;; NOTE: Assertions have been generated by update_lit_checks.py --all-items and should not be edited.
;; NOTE: This test was ported using port_passes_tests_to_lit.py and could be cleaned up.

;; RUN: foreach %s %t wasm-opt --code-pushing --tnh -all -S -o - | filecheck %s

(module
  (func $div (param $x i32) (param $y i32)
    (local $temp i32)
    (block $block
      ;; This division might trap (if x is 0). But with tnh we can assume that
      ;; won't happen, and push it past the condition.
      (local.set $temp
        (i32.div_u
          (i32.const 1)
          (local.get $x)
        )
      )
      (if
        (local.get $y)
        (return)
      )
      (drop
        (local.get $temp)
      )
    )
  )
)


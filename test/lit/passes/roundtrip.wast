    (local.get $y)
   )
  )
  (drop
   (ref.cast (ref stringview_iter)
    (local.get $z)
   )
  )
  ;; Here we do not need the cast.
  (drop
   (ref.cast (ref null stringview_wtf8)
    (local.get $x)
   )
  )
  (drop
   (ref.cast (ref null stringview_wtf16)
    (local.get $y)
   )
  )

  (func $tee
    (local $tuple (i32 i32))
    (local $tuple2 (i32 i32))
    (local.set $tuple
      (local.tee $tuple2
        (tuple.make
          (i32.const 1)
          (i32.const 2)
        )
      )
    )
    ;; Read the first tuple.
    (drop
      (tuple.extract 0
        (local.get $tuple)
      )
    )
    (drop
      (tuple.extract 1
        (local.get $tuple)
      )
    )
    ;; Read the second tuple.
    (drop
      (tuple.extract 0
        (local.get $tuple2)
      )
    )
    (drop
      (tuple.extract 1
        (local.get $tuple2)
      )

;; Adding a fourth type requires using yet another brand.
(module
  (rec
    ;; CHECK:      (type $a (struct))
    (type $a (struct))
    ;; CHECK:      (rec
    ;; CHECK-NEXT:  (type $1 (array (mut i8)))

    ;; CHECK:       (type $b (struct))
    (type $b (struct))

;; Now we can't differentiate by permutation because of automorphisms.
(module
  (rec
    ;; CHECK:      (rec
    ;; CHECK-NEXT:  (type $b1 (struct (field (ref $a1))))

    ;; CHECK:       (type $a1 (struct (field (ref $b1))))
    (type $a1 (struct (field (ref $b1))))
    (type $b1 (struct (field (ref $a1))))

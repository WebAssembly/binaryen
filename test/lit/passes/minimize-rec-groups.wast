  ;; CHECK:      (global $a1 (ref null $a1) (ref.null none))
  (global $a1 (ref null $a1) (ref.null none))
  ;; CHECK:      (global $a2 (ref null $a2) (ref.null none))
  (global $a2 (ref null $a2) (ref.null none))

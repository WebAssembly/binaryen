 ;; CHECK:      (func $undo-1 (param $var$1 i32) (param $var$2 i32)
 ;; CHECK-NEXT:  (local $var$5 i32)
 ;; CHECK-NEXT:  (local.set $var$2
 ;; CHECK-NEXT:   (local.get $var$1)
 ;; CHECK-NEXT:  )
 ;; CHECK-NEXT:  (local.set $var$2
 ;; CHECK-NEXT:   (i32.const 1)
 ;; CHECK-NEXT:  )
 ;; CHECK-NEXT:  (drop
 ;; CHECK-NEXT:   (local.get $var$1)
 ;; CHECK-NEXT:  )
 ;; CHECK-NEXT: )
 (func $undo-1 (param $var$1 i32) (param $var$2 i32)
  (local $var$5 i32)
  (local.set $var$2 ;; copy 1 to 2
   (local.get $var$1)
  )
  (local.set $var$2 ;; overwrite 2
   (i32.const 1)
  )
  (drop
   (local.get $var$1) ;; can't be changed to $var$2, as it changes
  )
 )

    (drop
      (ref.cast null $A
        (local.get $x)
      )
    )
    (drop
      (ref.cast null $B
        (local.get $x)
      )
    )
    (drop
      (ref.cast null $C1
        (local.get $x)
      )
    )
    (drop
      (ref.cast null $C2
        (local.get $x)
      )
    )
  )


  ;; NO_TNH-NEXT:    (local.get $x)
  ;; NO_TNH-NEXT:   )
  ;; NO_TNH-NEXT:  )
  ;; NO_TNH-NEXT: )
  (func $ref.test (param $x anyref)
    ;; This will return 0.
    (drop
      (ref.test $A
        (local.get $x)
      )
    )
    ;; This can test for a null.
    (drop
      (ref.test null $A
        (local.get $x)
      )
    )
  )


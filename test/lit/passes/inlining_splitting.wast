  ;; CHECK:      (func $br-to-toplevel (type $i32_=>_none) (param $x i32)
  ;; CHECK-NEXT:  (block $toplevel
  ;; CHECK-NEXT:   (if
  ;; CHECK-NEXT:    (local.get $x)
  ;; CHECK-NEXT:    (if
  ;; CHECK-NEXT:     (local.get $x)

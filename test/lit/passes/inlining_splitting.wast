  ;; CHECK-NEXT:      )
  ;; CHECK-NEXT:     )
  ;; CHECK-NEXT:    )
  ;; CHECK-NEXT:   )
  ;; CHECK-NEXT:  )
  ;; CHECK-NEXT: )
  (func $br-to-toplevel (param $x i32)
    (block $toplevel
      (if
        (local.get $x)
        (then
          (block
            (if
              (local.get $x)
              ;; A br to the toplevel block prevents us from outlining this code,
              ;; as we can't outline a br without its target.
              (then
                (br $toplevel)
              )
              (else
                (call $import)
              )
            )
          )
        )
      )
    )
  )


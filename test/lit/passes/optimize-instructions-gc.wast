  ;; CHECK-NEXT:      (ref.cast null none
  ;; CHECK-NEXT:       (local.get $a)
  ;; CHECK-NEXT:      )
  ;; CHECK-NEXT:     )
  ;; CHECK-NEXT:    )
  ;; CHECK-NEXT:    (unreachable)
  ;; CHECK-NEXT:   )
  ;; CHECK-NEXT:  )
  ;; CHECK-NEXT: )
  (func $ref-cast-static-null
    (local $a (ref null $A))
    ;; Casting nulls results in a null.
    (drop
      (ref.cast null $A
        (ref.null none)
      )
    )
    ;; A fallthrough works too.
    (drop
      (ref.cast null $B
        (local.tee $a
          (ref.null none)
        )
      )
    )
    ;; A non-null cast of a falling-though null will trap.
    (drop
      (ref.cast $A
        (local.tee $a
          (ref.null none)
        )
      )
    )
    ;; The prior two examples work even if the fallthrough is only later proven
    ;; to be null.
    (drop
      (ref.cast null $B
        (block (result (ref null $A))
          (ref.cast null none
            (local.get $a)
          )
        )
      )
    )
    (drop
      (ref.cast $B
        (block (result (ref null $A))
          (ref.cast null none
            (local.get $a)
          )
        )
      )
    )
  )


    (drop (f32.sub (f32.neg (local.get $a)) (local.get $b)))
    (drop (f64.sub (f64.neg (local.get $x)) (local.get $y)))
    ;; -x + y  ->  skip,  iff can't reorder
    (drop (f32.add (f32.neg (local.tee $a (f32.const 1.0))) (local.get $a)))

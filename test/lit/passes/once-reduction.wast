    )
    (global.set $once (i32.const 1))
    ;; A call to a non-"once" function.
    (call $other)
  )

  ;; CHECK:      (func $other (type $0)
  ;; CHECK-NEXT: )
  (func $other
  )

  ;; CHECK:      (func $caller (type $0)
  ;; CHECK-NEXT:  (call $other)
  ;; CHECK-NEXT:  (call $once)
  ;; CHECK-NEXT: )
  (func $caller
    ;; There is nothing to optimize here.
    (call $other)

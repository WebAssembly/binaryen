  ;; NOMNL-NEXT:    )
  ;; NOMNL-NEXT:    (local.set $1
  ;; NOMNL-NEXT:     (f64.const 0)
  ;; NOMNL-NEXT:    )
  ;; NOMNL-NEXT:    (ref.null none)
  ;; NOMNL-NEXT:   )
  ;; NOMNL-NEXT:  )
  ;; NOMNL-NEXT: )
  (func $simple
    ;; Other passes can remove such a trivial case of an unused allocation, but
    ;; we still optimize it.
    (drop

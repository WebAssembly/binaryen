        (drop (i32.const 1))
        (drop (i32.const 2))
        (i32.const 3)
      )
    )
    ;; condition
    (block $w
      (if (i32.const 0)
        (then
          (block
            (drop (i32.const 1))
            (drop (i32.const 2))
            (br_if $w (i32.const 3))
          )
        )
      )
      (drop (i32.const 1))
      (drop (i32.const 2))

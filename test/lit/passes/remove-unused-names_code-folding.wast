      (block $x3
        (br_table $side $x3 (i32.const 0))
        (if (i32.const 0)
          (then
            (block
              (drop (i32.const 1))
              (drop (i32.const 2))
              (br $x3)
            )
          )
        )
        (drop (i32.const 1))
        (drop (i32.const 2))
      )
    )
  )
